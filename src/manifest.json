{
	"manifest_version" : 2,
	"name" : "FrequencyWeave Tabs",
	"version" : "0.2",
	"author" : "kpuc <chris@ciar.org>",
	"icons" : {
		"48" : "icons/sortabs-48.png"
	},
	"description" : "Redistribute tabs by frequency of domain name",
	"browser_action" : {
		"default_icon" : {
			"16" : "icons/sortabs-16.png",
			"24" : "icons/sortabs-24.png",
			"48" : "icons/sortabs-48.png"
		},
		"default_title" : "FrequencyWeave",
		"default_popup" : "popup/picker.html",
		"browser_style" : true
	},
	"permissions" : [
		  "tabs"
	],
  "browser_specific_settings": {
    "gecko": {
      "id": "FrequencyWeave@example.com"
    }
  },
  "background": {
    "scripts": ["background.js"]
  }
}
